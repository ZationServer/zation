{
  "name": "{{name}/kc}",
  "version": "0.0.1",
  "description": "{{description}}",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",{{author}}{{git}}
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "test": "npm run build && mocha --exit -r ts-node/register test/**/*.ts",
    "check": "npm run build && MODE=c node dist/index.js",

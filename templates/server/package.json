  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "zation-server": "{{zationServerVersion}}"
  },
  "devDependencies": {
    "typescript": "{{typescriptVersion}}",
    "ts-node": "{{tsNodeVersion}}",
    "@types/node": "{{typesNodeVersion}}",
    "del-cli": "{{delCliVersion}}",
    "zation-assured": "{{zationAssuredVersion}}",
    "mocha": "{{mochaVersion}}",
    "@types/mocha": "{{typesMochaVersion}}"
  }
}